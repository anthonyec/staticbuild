    "@tsconfig/node17": "^1.0.0",
    "@types/mustache": "^4.1.2",
    "@types/node": "^17.0.25",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=17.0.1"
  }
}
