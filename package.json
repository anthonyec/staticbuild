{
  "name": "@anthonyec/staticbuild",
  "version": "0.0.5",
  "description": "Static site generator that isn't for you",
  "main": "dist/index.js",
  "repository": "https://github.com/anthonyec/staticbuild.git",
  "bin": {
    "staticbuild": "dist/bin/cli.js"
  },
